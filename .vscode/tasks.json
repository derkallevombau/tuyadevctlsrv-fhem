{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "compile",
			"group": {
                "kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
                "$tsc"
			],
			"label": "npm: compile",
			"detail": "tsc -p ./ --rootDir \"$npm_package_config_rootDir\" --outDir \"$npm_package_config_outDir\" --removeComments true"
		}
	]
}
